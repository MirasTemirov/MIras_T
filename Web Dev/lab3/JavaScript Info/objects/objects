1<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
</head>
<body>
<script>
  function min(a, b){
    if(a < b)
      return a;
    return b;
  }

  alert(min(45, 32));
</script>
</body>
</html>



2<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
</head>
<body>
<script>
  function isEmpty(object){
    for(let key in object){
      return false;
    }

    return true;
  }

  let schedule = {};

  alert( isEmpty(schedule) ); // true

  schedule["8:30"] = "get up";

  alert( isEmpty(schedule) ); // false
</script>
</body>
</html>

3<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
</head>
<body>
<script>
  function isEmpty(object){
    for(let key in object){
      return false;
    }

    return true;
  }

  let schedule = {};

  alert( isEmpty(schedule) ); // true

  schedule["8:30"] = "get up";

  alert( isEmpty(schedule) ); // false
</script>
</body>
</html>
