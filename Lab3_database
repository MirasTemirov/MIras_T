1.  DDL: CREATE , DROP , ALTER  DML: INSERT , UPDATE , DELETE , LOCK
2.  CREATE TABLE employees(
    emp_no int(10)
    birth_date DATE
    first_name VARCHAR(14)
    last_name VARCHAR(16)
    gender ENUM('M','F')
    hire_date DATE
    PRIMARY KEY (emp_no)
);
  CREATE TABLE departments (
    dep_no CHAR(4),
    dept_name VARCHAR(40),
    PRIMARY KEY(dep_no)
);
CREATE TABLE dept_emp(
    emp_no int(10),
    dep_no CHAR(4),
    from_date DATE,
    to_date DATE,
    FOREIGN KEY(dep_no) references departments(dep_no),
    FOREIGN KEY(emp_no) references employees(emp_no)
);
CREATE TABLE dept_manager(
    dep_no CHAR(4),
    emp_no int(10),
    from_date DATE,
    to_date DATE,
    FOREIGN KEY(dep_no) references departments(dep_no),
    FOREIGN KEY(emp_no) references employees(emp_no)
);
CREATE TABLE salaries(
    emp_no int(10),
    salary int(11),
    from_date DATE,
    to_date DATE
    FOREIGN KEY(emp_no) references employees(emp_no),
    FOREIGN KEY(from_date) references titles(from_date)
);
CREATE TABLE titles(
    emp_no int(10),
    title VARCHAR(50),
    from_date DATE,
    to_date DATE,
    PRIMARY KEY(from_date),
    PRIMARY KEY(title)
);
3.  CREATE TABLE student(
    full_name VARCHAR(10),
    age int(3),
    birth_date DATE,
    gender ENUM('M','F'),
    average_grade int(10),
    nationality VARCHAR(10),
    phone_number int(11),
    social_category VARCHAR(10),
    PRIMARY KEY(full_name)
);
CREATE TABLE instructors(
    full_name VARCHAR(10),
    speaking_languages VARCHAR(10),
    working_expirience VARCHAR(50),
    possibility_of_remote_lessons BIT DEFAULT 1,
    FOREIGN KEY (full_name) references student(full_name)
);
CREATE TABLE student_relatives(
    full_name VARCHAR(10),
    address int(10),
    phone_number int(11),
    position VARCHAR(10),
    FOREIGN KEY (phone_number) references student(phone_number),
    FOREIGN KEY (full_name) references student(full_name),
    PRIMARY KEY(adress)
);
CREATE TABLE social_data(
    school VARCHAR(10),
    graduation_data DATA,
    adress int(10),
    region VARCHAR(15),
    country VARCHAR(10),
    gpa FLOAT(10),
    honors VARCHAR(10),
    FOREIGN KEY (adress) references student_relatives(address),
);
4.
INSERT INTO employees
    (birth_date, first_name, last_name, gender, hire_date) VALUES (to_date(‘2003-12-20’, ‘yyyy-MM-dd’), ‘Miras’, ‘Temirov’, ‘Male’, to_date(NOW(), ‘yyyy-MM-dd’));

UPDATE salaries
   salary = 10000
WHERE emp_no = 3;

DELETE FROM employees
WHERE birth_date = "28-09-2022";
